{"manifest_version":2,
"name":"PlavorMindTools",
"author":"[http://exit.plavormind.tk:81/page/User:PlavorSeol PlavorSeol]",

"descriptionmsg":"plavormindtools-description",
"license-name":"AGPL-3.0-or-later",
"type":"other",
"url":"https://github.com/PlavorMind/PlavorMindTools",
"version":"1.3",

"AutoloadClasses":
  {"PlavorMindToolsHooks":"hooks.php"},
"AvailableRights":
  ["admin-accees",
  "autoconfirmed-access",
  "bureaucrat-access",
  "staff-access",
  "steward-access",
  "user-access"],
"config":
  {"PMTReplaceMsgForPlavorMind":
    {"value":false}
  },
"GroupPermissions":
  {"admin":
    {"admin-access":true},
  "autoconfirmed":
    {"autoconfirmed-access":true},
  "bureaucrat":
    {"bureaucrat-access":true},
  "staff":
    {"staff-access":true},
  "steward":
    {"steward-access":true},
  "user":
    {"user-access":true}
  },
"Hooks":
  {"BeforePageDisplay":"PlavorMindToolsHooks::onBeforePageDisplay",
  "BeforePageDisplayMobile":"PlavorMindToolsHooks::onBeforePageDisplayMobile",
  "MessageCache::get":"PlavorMindToolsHooks::onMessageCache_get"},
"MessagesDirs":
  {"PlavorMindTools":
    ["messages/main",
    "messages/permissions"]
  },
"ResourceFileModulePaths":
  {"localBasePath":"resources",
  "remoteExtPath":"PlavorMindTools/resources"},
"ResourceModules":
  {"bulma_notification":
    {"styles":
      {"bulma_notification.css":
        {"media":"screen"},
      "bulma_notification_fix.css":
        {"media":"screen"}
      }
    }
  },
"requires":
  {"MediaWiki":">=1.34"}
}
