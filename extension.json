{"name":"PlavorMindTools",
"author":"[http://central.plavormind.tk:81/page/User:PlavorSeol PlavorSeol]",

"config":
  {"PMTFeatureConfig":
    {"description":"Configurations for features",
    "value":
      {"BlueCategoryLinks":
        {"enable":false},
      "ManageOwnUserPages":
        {"enable":false},
      "NoActionsOnNonEditable":
        {"enable":false,
        "HideMoveTab":false},
      "ReplaceInterfaceMessages":
        {"enable":false,
        "EnglishSystemUsers":false}
      }
    }
  },
"descriptionmsg":"plavormindtools-description",
"license-name":"AGPL-3.0-or-later",
"manifest_version":2,
"requires":
  {"MediaWiki":">=1.35"},
"type":"other",
"url":"https://github.com/PlavorMind/PlavorMindTools",
"version":"3.1",

"AutoloadClasses":
  {"BlueCategoryLinksHooks":"includes/BlueCategoryLinksHooks.php",
  "NoActionsOnNonEditableHooks":"includes/NoActionsOnNonEditableHooks.php",
  "PlavorMindToolsHooks":"includes/PlavorMindToolsHooks.php",
  "ReplaceInterfaceMessagesHooks":"includes/ReplaceInterfaceMessagesHooks.php",
  "UserPageAccessHooks":"includes/UserPageAccessHooks.php"},
"AvailableRights":
  ["deleteownuserpages",
  "editotheruserpages",
  "moveownuserpages"],
"Hooks":
  {"getUserPermissionsErrors":
    ["NoActionsOnNonEditableHooks::ongetUserPermissionsErrors",
    "UserPageAccessHooks::ongetUserPermissionsErrors"],
  "MessageCache::get":"ReplaceInterfaceMessagesHooks::onMessageCache_get",
  "MovePageCheckPermissions":"UserPageAccessHooks::onMovePageCheckPermissions",
  "TitleIsAlwaysKnown":"BlueCategoryLinksHooks::onTitleIsAlwaysKnown",
  "TitleQuickPermissions":"UserPageAccessHooks::onTitleQuickPermissions"},
"MessagesDirs":
  {"PlavorMindTools":
    ["i18n",
    "i18n/additional",
    "i18n/replaceinterfacemessages/abusefilter-englishonly",
    "i18n/replaceinterfacemessages/babel-englishonly",
    "i18n/replaceinterfacemessages/core",
    "i18n/replaceinterfacemessages/core-englishonly",
    "i18n/replaceinterfacemessages/titleblacklist"]
  }
}
